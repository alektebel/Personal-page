    <style>
        /* Style for the navigation bar */
        .navbar {
            background-color: #333;
            width: 10%;
            position: fixed;
            height: 100%;
            overflow: hidden;
            padding: 20px 0;
        }
        
        /* Style for the navigation bar list items */
        .navbar ul {
            list-style: none;
            padding: 0;
        }
        
        /* Style for the navigation bar links */
        .navbar li {
            margin: 10px 0;
        }
        
        .navbar a {
            text-decoration: none;
            font-size: 18px;
            color: #fff;
        }
        
        /* Change the link color on hover */
        .navbar a:hover {
            color: #ddd;
        }
    </style>
